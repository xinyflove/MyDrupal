/* clearfix */
#navbar nav:after,.row:after,.center-wrapper:after,
.pane-content:after,.block-block-5:after,
.block-block-6:after,
.clearfix:after {
	display: block;
	clear: both;
	height: 0;
	line-height: 0;
	visibility: hidden;
	content: ".";
	font-size: 0;
}

#navbar nav,.row,.center-wrapper,
.pane-content,.block-block-5,
.block-block-6,
.clearfix {
	zoom: 1;
}

