/* frontpage slideshow */

#slideshow {
    position:relative;
    height:85px;
}

#slideshow DIV {
    position:absolute;
    top:0;
    left:0;
    z-index:8;
    opacity:0.0;
    height: 85px;
    background-color: #FFF;
}

#slideshow DIV.active {
    z-index:10;
    opacity:1.0;
}

#slideshow DIV.last-active {
    z-index:9; menu-mlid-394
}

#slideshow DIV IMG {
    height: 85px;
    display: block;
    border: 0;
    margin-bottom: 10px;
}

